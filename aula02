//operador logico 'NAO' ou 'negacao'

const valor1 = true
const valor2 = false
const valor3 = false

//verifica se o valor1 e o valor2 sao true
//se um deles for false, o resultado e false
console.log(valor1 && valor2 && valor3)//false

console.log(valor1 || valor2)


