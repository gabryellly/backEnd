import { Produtos } from "./exercicio9/produtos"

export class Produtos1 implements Produtos {
    doce: string;
    pao: string;
    arroz: string;

    constructor (doce: string, pao: string, arroz: string){
        this.doce = doce
        this.pao = pao 
        this.arroz = arroz
    }
}

    startEngine(): void {
        console.log('o ${this.doce} esta delicioso')

    startEngine(): void {
        console.log('o ${this.pao} esta macio')

    startEngine(): void {
        console.log('o ${this.arroz} esta otimo para cozinhar ')
    }

    
    
    doce.number(3)
    console.log('peguei ${number} ${this.doce}')

    pao.number(5)
    console.log('peguei ${number} ${this.pao}')

    arroz.number(1)
    console.log('peguei ${number} ${this.arroz')


        }
    
}

