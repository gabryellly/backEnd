export class Carro {
    marca: string;
    tipo: string;
    velocidade: number;

    constructor(marca: string, tipo: string, velocidade: number) {
        this.marca = marca;
        this.tipo = tipo;
        this.velocidade = 80;
    }

    correndo(Carro):void {
    console.log('o carro ${this.marca} esta correndo')
  }
}

