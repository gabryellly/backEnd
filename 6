aula 6 
export class Moto {
    //ATRIBUTOS
    marca: string;
    tipo: string;
    velocidade: number;

    //METODO
    constructor(marca: string, tipo: string) {
        this.marca = marca;
        this.tipo = tipo;
        this.velocidade = 100;
    }

    correndo(Moto):void {
    console.log('a moto ${this.marca} esta correndo');
  }
}

