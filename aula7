import { Carro } from "./carro";
import { Moto } from "./Moto";
let meuCarro:Carro = new ("toyota", "sedan", 80)
let minhaMoto:Moto = new ("honda", "titan", 70)

meuCarro.abrirPorta(3)
minhaMoto.darUmaGrau()

console.log(meuCarro.velocidade)
console.log(minhaMoto.velocidade)
console.log(meuCarro.marca)
console.log(minhaMoto.marca)
